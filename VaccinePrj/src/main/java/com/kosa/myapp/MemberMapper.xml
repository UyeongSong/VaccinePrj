<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.kosa.myapp.IMemberRepository">

	<select id="selectMember" parameterType="String" resultType="com.kosa.myapp.MemberVO">
	<![CDATA[
		select
			userId,
			password,
			name,
			email
		from member
		where userId=${userId}
	]]>	
	</select>










</mapper>